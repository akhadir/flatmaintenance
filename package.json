{
  "name": "flat-maintainanenace",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-multistep": "^5.1.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:watch": "jest --watchAll --colors --verbose",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "analyze-bundles": "start chrome bundle-report.html",
    "print-lint-config": "./node_modules/.bin/eslint --print-config *.js,*.ts,*.tsx",
    "eslint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "eslint-and-fix": "eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "lint": "node_modules/.bin/prettier --check src/**/*.{js,jsx,ts,tsx}",
    "lint-and-write": "node_modules/.bin/prettier -c src/**/*.{js,jsx,ts,tsx} --write",
    "e2e-tests": "npx codeceptjs run --steps --config ./e2e/config/dev.conf.js",
    "validate-locales": "node ./scripts/validate-locales.js",
    "postinstall": "bower cache clean && bower install",
    "sonar": "node sonar-project.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
