{
  "name": "flat-maintainanenace",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:watch": "jest --watchAll --colors --verbose",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "analyze-bundles": "start chrome bundle-report.html",
    "print-lint-config": "./node_modules/.bin/eslint --print-config *.js,*.ts,*.tsx",
    "eslint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "eslint-and-fix": "eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "lint": "node_modules/.bin/prettier --check src/**/*.{js,jsx,ts,tsx}",
    "lint-and-write": "node_modules/.bin/prettier -c src/**/*.{js,jsx,ts,tsx} --write",
    "e2e-tests": "npx codeceptjs run --steps --config ./e2e/config/dev.conf.js"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "moment": "^2.29.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-file-picker": "0.0.6",
    "react-scripts": "4.0.3",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "xlsx": "^0.16.9"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "26.0.22",
    "@types/lodash": "^4.14.149",
    "@types/node": "12.20.10",
    "@types/reach__router": "^1.3.4",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.2",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "^5.0.1",
    "eslint-config-airbnb": "^18.2.1"
  }
}
